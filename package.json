{
  "name": "elysia-protobuf",
  "version": "1.0.7",
  "author": "Toil",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "dependencies": {
    "@bufbuild/protobuf": "^2.5.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "elysia": "^1.3.4",
    "husky": "^9.1.7",
    "oxlint": "^1.1.0",
    "prettier": "^3.5.3",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "elysia": ">= 1.2.0"
  },
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./types/*": {
      "require": "./src/types/*.ts",
      "import": "./src/types/*.ts",
      "types": "./src/types/*.ts"
    }
  },
  "bugs": {
    "url": "https://github.com/ilyhalight/elysia-protobuf/issues"
  },
  "description": "Easy support protobuf integration for Elysia. To decode/encode we use @bufbuild/protobuf lib and schemas generated by ts-proto",
  "files": [
    "src"
  ],
  "homepage": "https://github.com/ilyhalight/elysia-protobuf",
  "keywords": [
    "elysia",
    "bun",
    "proto",
    "protobuf",
    "encode",
    "decode",
    "binary",
    "buf"
  ],
  "license": "MIT",
  "scripts": {
    "build:bun": "bun run ./scripts/build.ts",
    "lint": "bunx oxlint",
    "prepare": "husky"
  },
  "type": "module",
  "types": "./src/index.ts"
}
